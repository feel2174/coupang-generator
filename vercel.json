{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/generator.html",
      "dest": "/public/generator.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
